{
	"Arquos SnackBar Error": {
		"scope": "dart",
		"prefix": "arquosSB-error",
		"body": [
			"$0",
			"arquosSnackBar(",
				"\ttype: SnackBarType.error,",
				"\ttitle: '${1:Write title of error}',",
				"\tmessage: '${2: Write message of error}',",
		 	");"
		],
		"description": "Arquos SnackBar - Error"
	},
	"Arquos SnackBar Warning": {
